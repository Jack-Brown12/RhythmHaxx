{
    "tabWidth": 4,
    "useTabs": false,
    "singleQuote": false,
    "trailingComma": "es5",
    "semi": true,
    "printWidth": 100,
    "plugins": ["prettier-plugin-svelte", "prettier-plugin-tailwindcss"],
    "overrides": [
        { "files": "*.svelte", "options": { "parser": "svelte" } },
        { "files": "*.ts", "options": { "parser": "typescript" } },
        { "files": "*.js", "options": { "parser": "babel" } }
    ]
}
